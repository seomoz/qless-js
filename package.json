{
  "name": "qless-js",
  "version": "0.1.0",
  "private": false,
  "description": "Qless JavaScript Bindings",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint --ext .js bin lib test",
    "test": "jest --runInBand",
    "build": "git submodule update --init --recursive && make -C lib/qless-core clean all"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/seomoz/qless-js"
  },
  "author": "Moz",
  "contributors": [
    {
      "name": "Evan Battaglia",
      "email": "evan@moz.com"
    },
    {
      "name": "Dan Lecocq and other contributors of qless-core, qless-py, and qless Ruby",
      "email": "dlecocq@instructure.com"
    },
    {
      "name": "Myron Marston and other contritubutors of qless Ruby",
      "email": "myron@moz.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "commander": "^2.9.0",
    "double-ended-queue": "^2.1.0-0",
    "redis": "^2.7.1",
    "uuid": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "expect.js": "^0.3.1",
    "jest": "^19.0.2",
    "sinon": "^2.1.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!bin/**",
      "!index.js",
      "!**/node_modules/**",
      "!coverage/**",
      "!test/helper.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
